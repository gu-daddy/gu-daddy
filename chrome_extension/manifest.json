{
  "name": "Gu Daddy",
  "version": "1.0",
  "homepage_url": "https://gu-daddy.github.io/gu-daddy/",
  "description": "Replace all images with a picture of Mike Gu",
  "icons": { "32"  : "img/icon_32.jpeg",
             "48"  : "img/icon_48.jpeg",
             "64"  : "img/icon_64.jpeg",
             "128" : "img/icon_128.jpeg"},
  "browser_action": {
    "default_icon": "img/icon_48.jpeg",
    "default_title": "GU DADDY"
  },
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js", "gudaddy.js"],
    "persistent": false
  },
  "manifest_version": 2
}
